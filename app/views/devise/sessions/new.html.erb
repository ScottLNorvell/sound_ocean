<div id="sigin-box" class="devise-box">
 <%= link_to image_tag("sound_ocean.png", :border => 0), class: "sound_ocean" %> 

<h1>Sign In</h1>

<%# resource_class.omniauth_providers.each do |provider| %>
<%# link_to image_tag("btn-connect-sc-l.png", :border => 0), omniauth_authorize_path(resource_name, provider), :class => "sc-button" %><br />
<%# end %>

<%= form_for(resource, :as => resource_name, :url => session_path(resource_name)) do |f| %>
<!-- add class to div later to modify form. -->
  
  <div class="form-elements" >
  <%= f.email_field :email , :placeholder => "music@sea.com"%></div>

  <div class="form-elements" >
  <%= f.password_field :password, :placeholder => "Password" %></div>

  <% if devise_mapping.rememberable? -%>
    <div class="form-elements" ><%= f.check_box :remember_me %> <%= f.label :remember_me %></div>
  <% end -%>

  <div class="form-elements" ><%= f.submit "Sign in", class: "button-to" %></div>
<% end %>

<%= render "devise/shared/links" %>

</div>
